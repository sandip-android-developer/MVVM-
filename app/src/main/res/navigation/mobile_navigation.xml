<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.mvvmarcheitecture1.view.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_blankFragment"
            app:destination="@id/blankFragment" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.mvvmarcheitecture1.view.home.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_blankFragment_to_blankFragment2"
            app:destination="@id/blankFragment2"
            app:enterAnim="@anim/nav_default_enter_anim" />
        <action
            android:id="@+id/action_blankFragment_to_navigation_home"
            app:destination="@id/navigation_home" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment2"
        android:name="com.mvvmarcheitecture1.view.home.BlankFragment2"
        android:label="fragment_blank2"
        tools:layout="@layout/fragment_blank2"
        app:startDestination="@id/action_blankFragment2_to_blankFragment32">
        <action
            android:id="@+id/action_blankFragment2_to_blankFragment32"
            app:destination="@id/blankFragment3" />
        <action
            android:id="@+id/action_blankFragment2_to_blankFragment"
            app:destination="@id/blankFragment" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment3"
        android:name="com.mvvmarcheitecture1.view.home.BlankFragment3"
        android:label="fragment_blank3"
        tools:layout="@layout/fragment_blank3">
        <action
            android:id="@+id/action_blankFragment3_to_blankFragment2"
            app:destination="@id/blankFragment2" />
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.mvvmarcheitecture1.view.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_notifications" >
        <action
            android:id="@+id/action_navigation_dashboard_to_navigation_home"
            app:destination="@id/navigation_home" />
    </fragment>
    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.mvvmarcheitecture1.view.cart.CartFragment"
        android:label="@string/title_cart"
        tools:layout="@layout/fragment_cart" >
        <action
            android:id="@+id/action_navigation_cart_to_navigation_home"
            app:destination="@id/navigation_home" />
    </fragment>

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.mvvmarcheitecture1.view.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" >
        <action
            android:id="@+id/action_navigation_notifications_to_navigation_home"
            app:destination="@id/navigation_home" />
    </fragment>
</navigation>